package single_inheritace;

 public class employee {

    void salary(){
        System.out.println("salary is 70000");
    }
    
}
  
  class  hr extends employee {
    void  bonus(){
        System.out.println("bonus = 20000");
    }
}
    
    
class main{
    
   public static void main(String[] arg){
            
    hr myHr=new hr();
    myHr.bonus();
    myHr.salary();


   }
}
