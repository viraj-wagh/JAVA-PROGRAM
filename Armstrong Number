

import java.util.Scanner;
public class armstrong {

public static void main(String[] args){
Scanner sc=new Scanner(System.in);
System.out.println("Enetr a number");
int num=sc.nextInt();

int sum=0;
int temp=num;
int rem;

while(num>0){

rem=num%10;
num=num/10;
sum=sum+rem*rem*rem;
}
if(temp==sum){
   System.out.println("armstrong");
}else{
   System.out.println("not");
}

}
}
    
